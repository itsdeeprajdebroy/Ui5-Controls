{
  "_version": "1.65.0",
  "sap.app": {
    "id": "sap.raj.formatter",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.2",
      "toolsId": "69559008-5180-4d64-9e73-4edbfe80e543"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.raj.formatter.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "Currency": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      },
      "Date": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/date.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "sap.raj.formatter.view",
        "async": true,
        "viewPath": "sap.raj.formatter.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": "",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "FormatterView",
          "pattern": "FormatterView",
          "target": [
            "TargetView2"
          ]
        },
        {
          "name": "TableOne",
          "pattern": "TableOne",
          "target": [
            "TargetView3"
          ]
        },
        {
          "name": "TableTwo",
          "pattern": "TableTwo",
          "target": [
            "TargetView4"
          ]
        },
        {
          "name": "TableCurr",
          "pattern": "TableCurr",
          "target": [
            "TargetView5"
          ]
        },
        {
          "name": "TableThree",
          "pattern": "TableThree",
          "target": [
            "TargetView6"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "TargetView2": {
          "id": "FormatterView",
          "name": "FormatterView"
        },
        "TargetView3": {
          "id": "TableOne",
          "name": "TableOne"
        },
        "TargetView4": {
          "id": "TableTwo",
          "name": "TableTwo"
        },
        "TargetView5": {
          "id": "TableCurr",
          "name": "TableCurr"
        },
        "TargetView6": {
          "id": "TableThree",
          "name": "TableThree"
        }
      }
    },
    "rootView": {
      "viewName": "sap.raj.formatter.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
