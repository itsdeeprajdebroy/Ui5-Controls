<core:FragmentDefinition 
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Dialog id="_IDGenDialog3" >
        <customHeader>
            <Bar id="_IDGenBar2" >
                <contentMiddle>
                    <Title id="_IDGenTitle2" text="Group"/>
                </contentMiddle>
            </Bar>
        </customHeader>
        <content>
            <VBox id="_IDGenVBox4" class="sapUiSmallMargin">
                <Toolbar id="_IDGenToolbar4" >
                    <Label id="_IDGenLabel1" text="Group By"/>
                </Toolbar>
                <Select id="_IDGenSelect2" >
                    <items>
                        <core:Item id="_IDGenItem1" key="Department" text="Department"/>
                        <core:Item id="_IDGenItem7" key="City" text="City"/>
                    </items>
                </Select>
            </VBox>
        </content>
        <beginButton>
            <Button id="_IDGenButton6" text="Apply" press="onGroupApply"/>
        </beginButton>
        <endButton>
            <Button id="_IDGenButton7" text="Cancel" press="onDialogClose" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>