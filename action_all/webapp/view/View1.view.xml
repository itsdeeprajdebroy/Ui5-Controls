<mvc:View controllerName="sap.po.actionall.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <VBox id="_IDGenVBox" class="sapUiSmallMargin">
            <Button id="_IDGenButton" text="List-Filter" icon="sap-icon://list" press=".onClickList"/>
            <Button id="_IDGenButtonSorter" text="List-Sorter" icon="sap-icon://sort" press=".onClickSort"/>
            <Button id="_IDGenButtonAll" text="All in One" icon="sap-icon://collapse-group" press=".onClickNext"/>
            <Button id="_IDGenButton_inView" text="All in One" icon="sap-icon://expand" press=".onClickPlus"/>
        </VBox>
    </Page>
</mvc:View>